# Utility
alias cls='clear'
alias :q='exit'

# LS commands
alias ll='ls -lF'
alias ls='ls -F'
alias la='ls -a1'
alias vm='homestead ssh'

# Git
alias gpstaging='git push staging staging'
alias gpproduction='git push production production'
alias gchm='git checkout master'
alias gchs='git checkout staging'
alias gchp='git checkout production'

# Composer
alias compda='composer dumpautoload -o'
